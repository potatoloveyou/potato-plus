{
	"compilerOptions": {
		"target": "ES6",
		"module": "commonjs",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"moduleResolution": "node" // 确保模块解析方式为 node
	},
	"include": ["src"],
	"exclude": ["node_modules"]
}
